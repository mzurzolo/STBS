import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
%matplotlib inline

def main():
    """Run the program."""
    x = [i for i in range(100)]
    print(x)
    y = [i for i in range(100)]
    print(y)

    df = pd.DataFrame({'x': x,
                       'y': y})

    df.plot(x='x', y='y')

if __name__ == "__main__":
    main()
