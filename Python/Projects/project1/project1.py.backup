import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
%matplotlib inline

def main():
    """Run the program."""
    x = np.array([i for i in range(10)])
    print(x)
    y = np.array([i for i in range(10)])
    print(y)

    df = pd.DataFrame({'x': x,
                       'y': y})

    plt.figure()
    plt.plot(df['x'], df['y'])
    plt.show()


if __name__ == "__main__":
    main()
